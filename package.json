{
  "name": "saleor-alt-storefront",
  "displayName": "Saleor Alt Storefront",
  "author": {
    "name": "Khalifa Lame",
    "email": "khalibloo@gmail.com"
  },
  "license": "MIT",
  "private": true,
  "version": "0.0.1",
  "scripts": {
    "analyze": "cross-env ANALYZE=1 umi build",
    "start": "umi dev",
    "start:no-mock": "cross-env MOCK=none umi dev",
    "start:no-ui": "cross-env UMI_UI=none umi dev",
    "build": "umi build",
    "build:prod": "cross-env UMI_ENV=prod umi build",
    "codegen": "apollo codegen:generate --target=typescript types --globalTypesFile=src/globalTypes.ts",
    "codegen:watch": "npm run codegen -- --watch",
    "prettier": "prettier --write src/**/*.{js,jsx,tsx,ts,less,md,json}",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage",
    "test:watch": "umi-test --watch"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "engines": {
    "node": "^12.13.0",
    "npm": "^6.9.1",
    "yarn": "^1.22.0"
  },
  "dependencies": {
    "@apollo/react-hooks": "^3.1.3",
    "@sentry/browser": "^5.15.4",
    "@umijs/preset-react": "1.x",
    "@umijs/test": "^3.0.14",
    "antd": "^4.0.4",
    "apollo-boost": "^0.4.7",
    "apollo-link-logger": "^1.2.3",
    "apollo-link-retry": "^2.2.15",
    "apollo-link-token-refresh": "^0.2.7",
    "apollo-upload-client": "^12.1.0",
    "classnames": "^2.2.6",
    "dompurify": "^2.0.8",
    "draftjs-to-html": "^0.9.1",
    "dva-logger": "^1.0.0",
    "graphql": "^14.6.0",
    "jwt-decode": "^2.2.0",
    "lint-staged": "^10.0.7",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "prettier": "^1.19.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-lazyload": "^2.6.6",
    "react-truncate-html": "^0.1.7",
    "umi": "^3.0.14",
    "yorkie": "^2.0.0"
  },
  "devDependencies": {
    "@types/jest": "^25.1.4",
    "core-js": "^3.6.4",
    "cross-env": "^7.0.2",
    "html-critical-webpack-plugin": "^2.1.0",
    "import-sort-cli": "^6.0.0",
    "jest-playwright-preset": "^0.1.1",
    "playwright": "^0.12.1",
    "typescript": "^3.8.3"
  }
}
